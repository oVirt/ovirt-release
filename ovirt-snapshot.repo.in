[copr:copr.fedorainfracloud.org:ovirt:ovirt-master-snapshot]
name=Copr repo for ovirt-master-snapshot owned by ovirt
baseurl=https://download.copr.fedorainfracloud.org/results/ovirt/ovirt-master-snapshot/centos-stream-$releasever-$basearch/
type=rpm-md
skip_if_unavailable=True
gpgcheck=1
gpgkey=https://download.copr.fedorainfracloud.org/results/ovirt/ovirt-master-snapshot/pubkey.gpg
repo_gpgcheck=0
enabled=1
enabled_metadata=1

[ovirt-appliance-master-snapshot]
name=oVirt appliance with ovirt-master-snapshot content
baseurl=https://resources.ovirt.org/repos/ovirt/github-ci/ovirt-appliance/el$releasever/
gpgcheck=0
enabled=1

[ovirt-node-master-snapshot]
name=oVirt Node with ovirt-master-snapshot content
baseurl=https://resources.ovirt.org/repos/ovirt/github-ci/ovirt-node-ng-image/el$releasever/
gpgcheck=0
enabled=1
