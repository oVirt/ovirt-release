 python3-oslo-service
 python3-oslo-utils
 python3-paste
 python3-paste-deploy
 python3-pyparsing
 python3-retrying
 python3-tempita
 python3-yappi
 python3-eventlet
 python3-fasteners
 python3-oslo-privsep
 python3-msgpack
