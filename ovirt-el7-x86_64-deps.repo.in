[ovirt-@OVIRT_SLOT@-epel]
name=Extra Packages for Enterprise Linux 7 - $basearch
#baseurl=http://download.fedoraproject.org/pub/epel/7/$basearch
mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=epel-7&arch=$basearch
failovermethod=priority
enabled=1
includepkgs=
 archaius-core,
 epel-release,
 hystrix-core,
 hystrix-metrics-event-stream,
 ioprocess,
 jackson-core,
 jctools,
 libtomcrypt,
 libtommath,
 livecd-tools,
 mom,
 novnc,
 objenesis,
 ovirt-guest-agent*,
 protobuf-java,
 puppet,
 python2-crypto,
 python2-ecdsa,
 python2-paramiko,
 python34*,
 python-argparse,
 python-cheetah,
 python-cpopen,
 python-daemon,
 python-ecdsa,
 python-inotify,
 python-ioprocess,
 python-IPy,
 python-kitchen,
 python-lockfile,
 python-markdown,
 python-ordereddict,
 python-paramiko,
 python-ply,
 python-pthreading,
 python-simplejson,
 python-uinput,
 python-websockify,
 rubygem-rgen,
 rxjava,
 safelease,
 spice-html5,
 userspace-rcu

gpgcheck=1
gpgkey=https://dl.fedoraproject.org/pub/epel/RPM-GPG-KEY-EPEL-7

[ovirt-@OVIRT_SLOT@-centos-gluster310]
name=CentOS-7 - Gluster 3.10
baseurl=http://mirror.centos.org/centos/7/storage/$basearch/gluster-3.10/
gpgcheck=1
enabled=1
gpgkey=https://raw.githubusercontent.com/CentOS-Storage-SIG/centos-release-storage-common/master/RPM-GPG-KEY-CentOS-SIG-Storage

[ovirt-@OVIRT_SLOT@-virtio-win-latest]
name=virtio-win builds roughly matching what will be shipped in upcoming RHEL
baseurl=http://fedorapeople.org/groups/virt/virtio-win/repo/latest
enabled=1
skip_if_unavailable=1
gpgcheck=0

[centos-qemu-ev-release]
name=CentOS-7 - QEMU EV
baseurl=http://mirror.centos.org/centos/7/virt/$basearch/kvm-common/
gpgcheck=1
enabled=1
gpgkey=https://git.centos.org/raw/rpms/centos-release-virt-common.git/c7-extras/SOURCES!RPM-GPG-KEY-CentOS-SIG-Virtualization

[centos-ovirt42-testing]
name=CentOS-7 - oVirt 4.2
baseurl=http://cbs.centos.org/repos/virt7-ovirt-42-testing/$basearch/os/
gpgcheck=0
enabled=1

[centos-ovirt-common-testing]
name=CentOS-7 - oVirt common
baseurl=http://cbs.centos.org/repos/virt7-ovirt-common-testing/$basearch/os/
gpgcheck=0
enabled=1

[opstools7-fluentd-012-testing]
name=CentOS-7 - OpsTools - fluentd testing repo
baseurl=https://cbs.centos.org/repos/opstools7-fluentd-012-testing/$basearch/os
gpgcheck=0
enabled=1

[opstools7-perfmon-common-testing]
name=CentOS-7 - OpsTools - perfmon testing repo
baseurl=https://cbs.centos.org/repos/opstools7-perfmon-common-testing/$basearch/os
gpgcheck=0
enabled=1

[opstools7-sensu-common-testing]
name=CentOS-7 - OpsTools - sensu testing repo
baseurl=https://cbs.centos.org/repos/opstools7-sensu-common-testing/$basearch/os
gpgcheck=0
enabled=1

[opstools7-common-testing]
name=CentOS-7 - OpsTools - common testing repo
baseurl=https://cbs.centos.org/repos/opstools7-common-testing/$basearch/os
gpgcheck=0
enabled=1

[centos-opstools-release]
name=CentOS-7 - OpsTools - release
baseurl=http://mirror.centos.org/centos/7/opstools/$basearch/
gpgcheck=0
enabled=1
skip_if_unavailable=1

[sac-gdeploy]
name=Copr repo for gdeploy owned by sac
baseurl=https://copr-be.cloud.fedoraproject.org/results/sac/gdeploy/epel-7-$basearch/
type=rpm-md
skip_if_unavailable=True
gpgcheck=1
gpgkey=https://copr-be.cloud.fedoraproject.org/results/sac/gdeploy/pubkey.gpg
repo_gpgcheck=0
enabled=1
enabled_metadata=1
